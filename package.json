{
  "name": "tsfy",
  "version": "0.2.1",
  "description": "TypeScript For You: Useful type-safe utility functions and types",
  "author": "Brian Shao <brian@jzshao.com>",
  "license": "MIT",
  "repository": "https://github.com/cydq/tsfy",
  "type": "module",
  "keywords": [
    "typescript",
    "utility",
    "functional",
    "option",
    "result"
  ],
  "files": [
    "dist/"
  ],
  "main": "dist/core/index.cjs",
  "types": "dist/core/index.d.cts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/core/index.d.ts",
        "default": "./dist/core/index.js"
      },
      "require": {
        "types": "./dist/core/index.d.cts",
        "default": "./dist/core/index.cjs"
      }
    },
    "./collections": {
      "import": {
        "types": "./dist/collections/index.d.ts",
        "default": "./dist/collections/index.js"
      },
      "require": {
        "types": "./dist/collections/index.d.cts",
        "default": "./dist/collections/index.cjs"
      }
    },
    "./functions": {
      "import": {
        "types": "./dist/functions/index.d.ts",
        "default": "./dist/functions/index.js"
      },
      "require": {
        "types": "./dist/functions/index.d.cts",
        "default": "./dist/functions/index.cjs"
      }
    },
    "./random": {
      "import": {
        "types": "./dist/random/index.d.ts",
        "default": "./dist/random/index.js"
      },
      "require": {
        "types": "./dist/random/index.d.cts",
        "default": "./dist/random/index.cjs"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/types/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.cts",
        "default": "./dist/types/index.cjs"
      }
    }
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "tsup --clean",
    "lint": "pnpm run lint:eslint && pnpm run lint:types",
    "lint:eslint": "eslint . --ext .ts",
    "lint:types": "tsc",
    "test": "jest",
    "prepack": "pnpm run build"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "terser": "^5.27.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsup": "^7.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "type-fest": "^4.10.2"
  }
}
